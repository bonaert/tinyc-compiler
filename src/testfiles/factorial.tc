int factorial(int n) {
    int res = 1;
    int factor = n;
    while (factor > 1) {
        res = res * factor;
        factor = factor - 1;
    }
    return res;
}

int main() {
    int i = 0;
    while (i < 10) {
        
        write "factorial(";
        write i;
        write ") = ";
        write factorial(i);
        write "\n";
        
        i = i + 1;
    }

    return 1;
}