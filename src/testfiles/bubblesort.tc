int printArray(int[10] n, int count, int withIndex) {
    int a = 0;
    while (a < count) {
        if (withIndex != 0) {
            write a;
            write ':';
        }  
        write n[a];
        write '\n';
        a = a + 1;
    }
}

int bubblesort(int[1] numbers, int count) {
    int i = count - 1;
    while (i >= 0) {
        int j = 0;
        
        while(j < i){
            if (numbers[j] > numbers[j+1]) {
                int temp = numbers[j];
                numbers[j] = numbers[j+1];
                numbers[j+1] = temp;
            }
            j = j + 1;      
        }
        i = i - 1;
    }
}

