int printArray(int[10] n, int count, int withIndex) {
    int a = 0;
    while (a < count) {
        if (withIndex != 0) {
            write a;
            write ':';
        }  
        write n[a];
        write '*';
        a = a + 1;
    }

    // TODO: deal with the situation where there's no return value
    return 0;
}

int bubblesort(int[1] numbers, int count) {
    int temp;
    int i;
    int j;
   

    // This is the main logic of bubble sort algorithm 
    i = count - 1;
    while (i >= 0) {
        j = 0;
        
        while(j < i){
            if (numbers[j] > numbers[j+1]) {
                temp = numbers[j];
                numbers[j] = numbers[j+1];
                numbers[j+1] = temp;
            }
            j = j + 1;      
        }
        i = i - 1;
    }

    return 0;
}

