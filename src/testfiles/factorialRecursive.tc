int factorialRecursive(int n) {
    int res = 1;
    if (n > 1) {
        res = n * factorialRecursive(n - 1);
    }
    return res;
}

int main() {
    int i = 0;
    while (i < 10) {
        
        write "factorial(";
        write i;
        write ") = ";
        write factorialRecursive(i);
        write "\n";
        
        i = i + 1;
    }

    return 1;
}